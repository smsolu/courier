{% set bag = app.session.flashbag.all %}
{% if bag is not empty %}
        {% for label, flashes in bag %}
            {% for flash in flashes %}
                <div class="alert alert-{{ label }}">
                    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                    {{ flash|raw }}
                </div>
            {% endfor %}
        {% endfor %}
{%endif%}
