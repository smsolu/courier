{% block content -%}
{#<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">#}
{{ form_start(form) }}
<div class="panel panel-primary">
    <div class="panel-heading"><h3 class="panel-title">{%trans%}sec_user_datoscuenta{%endtrans%}</h3></div>
    <div class="panel-body">
            <div class="form-group col-xs-12 col-sm-12 {% if form.email.vars.valid==false %}has-error{% endif %}">
                {{ form_label(form.username) }}
                {{ form_widget(form.username) }}
                {{ form_errors(form.username) }}
            </div>
            <div class="form-group col-xs-12 col-sm-12 {% if form.email.vars.valid==false %}has-error{% endif %}">
                {{ form_label(form.email) }}
                {{ form_widget(form.email) }}
                {{ form_errors(form.email) }}
            </div>
            <div class="form-group col-xs-6 col-sm-6 {% if form.plainPassword.first.vars.valid==false %}has-error{% endif %}">
                {{ form_label(form.plainPassword.first) }}
                {{ form_widget(form.plainPassword.first) }}
                {{ form_errors(form.plainPassword.first) }}
            </div>
            <div class="form-group col-xs-6 col-sm-6 {% if form.plainPassword.second.vars.valid==false %}has-error{% endif %}">
                {{ form_label(form.plainPassword.second) }}
                {{ form_widget(form.plainPassword.second) }}
                {{ form_errors(form.plainPassword.second) }}
            </div>
    </div>
</div>
    <div class="panel panel-primary">
        <div class="panel-heading"><h3 class="panel-title">{%trans%}menu_estudio_datosestudio{%endtrans%}</h3></div>
        <div class="panel-body">
            <div class="form-group col-xs-12 col-sm-12 {% if form.estudio.nombre.vars.valid==false %}has-error{% endif %}">
                {{ form_label(form.estudio.nombre) }}
                {{ form_widget(form.estudio.nombre) }}
                {{ form_errors(form.estudio.nombre) }}
            </div>
        </div>
    </div>
    <div>
        <input class="btn btn-primary" type="submit" value="{{ 'Registrar' }}" />
    </div>
{#    </form>#}
{{ form_end(form) }}
    

{% endblock %}