{% extends '::base.html.twig' %}
{% block aside_breadcrumb %}
    {% set breadcrumb = knp_menu_get('AppBundle:BreadcrumbBuilder:expedienteShowMenu', [],{'expediente' : expediente} ) %}
    {{ knp_menu_render(breadcrumb, {'allow_safe_labels': true} ) }}
{% endblock %}
{% block aside_left %}
    {{ include('AppBundle:Expediente:submenu/sm_general.html.twig') }}
{% endblock %}
{% block body_top %}
    {{ include('AppBundle:Expediente:cabecera.html.twig') }}
    {{ include('AppBundle:Expediente:subnav/subnav.html.twig') }}
{% endblock %}
{% block body %}    
    <div class="col-xs-12">
        {{form(formPrincipal)}}
    </div>
    <div class="col-xs-12">
        {{form(formSecundario)}}
    </div>
    <div class="col-md-10">
        {{ render (controller("ListViewBundle:ListView:ListView", {'request': request, 'listview': list}))}}
    </div>
    <div class="col-xs-12">
        <a class="btn btn-primary" href="{{path('expediente_tipoproceso_delete',{'idExpedienteTipoProc':'all', 'id': expediente.id})}}">Borrar todos </a>
    </div>
{% endblock %}